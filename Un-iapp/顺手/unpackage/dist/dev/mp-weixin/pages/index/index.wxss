	height: 400rpx;
	margin: 0 auto;
	border-radius: 45rpx;
	background-color: #D9E9F6;
	border: #8AA9C5 solid 3px;
}
.icons {
	display: flex;
	justify-content: center;
	align-items: center;
	flex-wrap: wrap;
	width: 650rpx;
	height: 400rpx;
	margin: 0 auto;
}
.icons view {
	display: flex;
	flex-direction: column;
	justify-content: center;
	align-items: center;
	width: 150rpx;
	height: 150rpx;
}
.icons view image {
	width: 100rpx;
	height: 100rpx;
}
